        .pricing-card:hover {
            transform: scale(1.03);
            border-color: var(--primary);
        }

        .section-title {
            text-transform: uppercase;
            font-weight: 800;
            letter-spacing: 2px;
            color: var(--primary);
            margin-bottom: 1rem;
        }

        .lowercase-text {
            text-transform: lowercase;
            color: var(--text-gray);
            font-weight: 400;
        }
